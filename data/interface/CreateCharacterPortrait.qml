    property string selectedPortrait

    onPortraitsChanged: updatePortraits()
    onPlayerRaceChanged: updatePortraits()
    onPlayerGenderChanged: updatePortraits()

    ListModel {
        id: portraitModel
    }

    function updatePortraits() {
        portraitModel.clear();
        portraits.forEach(function (portrait) {
            if (!portrait.race || !portrait.gender)
                return;

            if (!showAllGenders.checked && portrait.gender != playerGender)
                return;

            if (!showAllRaces.checked && portrait.race != playerRace)
                return;

            portraitModel.append({
                id: portrait.id,
                large: portrait.large,
                medium: portrait.medium,
                small: portrait.small,
                gender: portrait.gender,
                race: portrait.race
            });
        });
    }

    GridView {
        id: portraitView

        anchors.top: parent.top
        anchors.left: parent.left
        anchors.right: parent.right
        anchors.bottom: footer.top
        anchors.bottomMargin: 10

        Keys.onReturnPressed: {
            selectedPortrait = portraitModel.get(portraitView.currentIndex).id;
        }

        model: portraitModel
        cellHeight: 64
        cellWidth: 64
        delegate: Item {
            width: portraitView.cellWidth
            height: portraitView.cellHeight
            Image {
                anchors.fill: image
                anchors.margins: -3
                source: 'ImageGlow.png'
                visible: selectedPortrait == model.id
            }
            Image {
                id: image
                source: '../art/interface/portraits/' + model.small
                anchors.centerIn: parent
            }
            MouseArea {
                anchors.fill: parent
                onClicked: {
                    portraitView.currentIndex = index;
                    selectedPortrait = model.id;
                }
            }
        }
        highlight: Rectangle {
            color: '#00000000'
            border.color: 'white'
            border.width: 1
            radius: 5
        }
        clip: true
        boundsBehavior: "StopAtBounds"
        focus: true
        highlightFollowsCurrentItem: true
    }

    Row {
        id: footer
        spacing: 5

        anchors.bottom : parent.bottom
        anchors.horizontalCenter: parent.horizontalCenter

        Checkbox {
            id: showAllGenders
            onCheckedChanged: updatePortraits()
        }
        StandardText {
            text: 'Show both genders'
        }
        Item {
            width: 10
            height: 1
        }

        Checkbox {
            id: showAllRaces
            onCheckedChanged: updatePortraits()
        }
        StandardText {
            text: 'Show all races'
        }
    }

}
